generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model taxis {
  id    Int    @id(map: "Taxi_pkey")
  plate String @db.VarChar
}
model trajectories {
  taxi_id   Int
  latitude  Float
  longitude Float
  id        Int      @id @default(autoincrement())
  date      DateTime @db.Timestamp(6)

taxi taxis @relation(fields:[taxi_id].references: [id], ondelete:Cascade)
}

model users {
  id       Int    @id @default(autoincrement()) //indica q es primary key
  name     String
  email    String
  password String

}
